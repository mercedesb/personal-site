  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "contentful": "^5.1.3",
    "dotenv": "^5.0.1",
    "markdown-it": "^8.4.1",
    "moment": "^2.22.1",
    "normalize.css": "^8.0.0",
